        {
          "id": "streetNo",
          "label": "Street No",
          "type": "text",
          "required": false,
          "bindingProperty": "application.Premises.StreetNo"
        }
      ]
    },
    {
      "sectionTitle": "Personal Details",
      "fields": [
        {
          "id": "homeAddress",
          "label": "Home Address",
          "type": "text",
          "required": false,
          "bindingProperty": "application.PartII.HomeAddress"
        },
        {
          "id": "dateOfBirth",
          "label": "Date of Birth",
          "type": "date",
          "required": false,
          "bindingProperty": "application.PartII.DateOfBirth"
