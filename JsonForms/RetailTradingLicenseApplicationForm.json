{
  "formId": "retail-trading-license-application-form",
  "formName": "Retail Trading License Application Form",
  "sections": [
    {
      "sectionTitle": "Applicant Info",
      "fields": [
        {
          "id": "fullName",
          "label": "Full name of Applicant",
          "type": "text",
          "required": true,
          "bindingProperty": "application.FullName"
        },
        {
          "id": "postalAddress",
          "label": "Postal address",
          "type": "text",
          "required": false,
          "bindingProperty": "application.PostalAddress"
        },
        {
          "id": "phoneNumber",
          "label": "Phone No",
          "type": "text",
          "required": false,
          "bindingProperty": "application.PhoneNumber"
        }
      ]
    },
    {
      "sectionTitle": "Business Details",
      "fields": [
        {
          "id": "businessName",
          "label": "Business Name",
          "type": "text",
          "required": false,
          "bindingProperty": "application.BusinessName"
        },
        {
          "id": "descriptionOfGoods",
          "label": "Description of goods to be sold",
          "type": "textarea",
          "required": false,
          "bindingProperty": "application.DescriptionOfGoods"
        }
      ]
    },
    {
      "sectionTitle": "Premises Info",
      "fields": [
        {
          "id": "standNo",
          "label": "Stand No",
          "type": "text",
          "required": false,
          "bindingProperty": "application.Premises.StandNo"
        },
        {
          "id": "shopNo",
          "label": "Shop No",
          "type": "text",
          "required": false,
          "bindingProperty": "application.Premises.ShopNo"
